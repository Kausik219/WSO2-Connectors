<?xml version="1.0" encoding="UTF-8"?><proxy xmlns="http://ws.apache.org/ns/synapse" name="Scenario3" startOnLoad="true" statistics="disable" trace="disable" transports="http,https">
    <target>
        <inSequence>
            <property expression="json-eval($.amoCRMapiUrl)" name="amoCRMapiUrl"/>
            <property expression="json-eval($.amoCRMapiKey)" name="amoCRMapiKey"/>
            <property expression="json-eval($.amoCRMapiVersion)" name="amoCRMapiVersion"/>
            <property expression="json-eval($.email)" name="email"/>
            <property expression="json-eval($.amoCRMid)" name="amoCRMid"/>
            <amoCRM.init>
                <amoCRMapiUrl>{$ctx:amoCRMapiUrl}</amoCRMapiUrl>
                <amoCRMapiKey>{$ctx:amoCRMapiKey}</amoCRMapiKey>
                <amoCRMapiVersion>{$ctx:amoCRMapiVersion}</amoCRMapiVersion>
                <email>{$ctx:email}</email>
            </amoCRM.init>
            <property expression="json-eval($.apiUrl)" name="apiUrl"/>
         <property expression="json-eval($.userName)" name="userName"/>
         <property expression="json-eval($.apiVersion)" name="apiVersion"/>
         <property expression="json-eval($.pipelineKey)" name="pipelineKey"/>
         <property expression="json-eval($.boxKey)" name="boxKey"/>
         <property expression="json-eval($.name)" name="name"/>
         <property expression="json-eval($.description)" name="description"/>
         <property expression="json-eval($.dueDate)" name="dueDate"/>
         <property expression="json-eval($.text)" name="text"/>
         <streak.init>
            <apiUrl>{$ctx:apiUrl}</apiUrl>
            <userName>{$ctx:userName}</userName>
            <apiVersion>{$ctx:apiVersion}</apiVersion>
            <boxKey>{$ctx:boxKey}</boxKey>
         </streak.init>
         <streak.createTask>
            <dueDate>{$ctx:dueDate}</dueDate>
            <text>{$ctx:text}</text>
         </streak.createTask>
            <amoCRM.getTasks>
                <amoCRMid>{$ctx:amoCRMid}</amoCRMid>
            </amoCRM.getTasks>
            <property expression="json-eval($.response.tasks[0].task_type)" name="taskType"/>
            <property expression="json-eval($.response.tasks[0].complete_till)" name="comDate"/>
            <property expression="get-property('taskType')" name="taskName"/>
            <streak.init>
                <apiUrl>{$ctx:apiUrl}</apiUrl>
                <userName>{$ctx:userName}</userName>
                <apiVersion>{$ctx:apiVersion}</apiVersion>
                <boxKey>{$ctx:boxKey}</boxKey>
            </streak.init>
            <streak.createTask>
                <dueDate>{$ctx:dueDate}</dueDate>
                <text>{$ctx:text}</text>
            </streak.createTask>
            <respond/>
        </inSequence>
        <outSequence>
            <send/>
        </outSequence>
    </target>
    <description/>
</proxy>
                                
